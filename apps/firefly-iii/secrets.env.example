# Example secrets file for Firefly III
# Copy this to secrets.env and fill in your values

# Application key (generate with: openssl rand -base64 32)
APP_KEY=base64:CHANGE_ME_GENERATE_WITH_OPENSSL

# Database configuration
DB_HOST=postgres
DB_PORT=5432
DB_CONNECTION=pgsql
DB_DATABASE=firefly
DB_USERNAME=firefly
DB_PASSWORD=CHANGE_ME_SECURE_PASSWORD
